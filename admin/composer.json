{
    "name": "your-vendor/voting-system",
    "description": "A simple PHP voting system",
    "type": "project",
    "require": {
        "php": ">=7.4",
        "monolog/monolog": "^2.0",
        "guzzlehttp/guzzle": "^7.0",
        "doctrine/dbal": "^3.0",
        "symfony/var-dumper": "^5.0",
        "phpoffice/phpspreadsheet": "^3.3"
    },
    "autoload": {
        "psr-4": {
            "VotingSystem\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "echo 'Composer install completed!'"
        ]
    },
    "config": {
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
